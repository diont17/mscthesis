  keywords = {MR susceptometry,T2 oximetry,T2 relaxometry},
  pages = {2290--2296},
  file = {/home/dion/Zotero/storage/FC9BJAEI/Langham et al. - 2018 - In vivo whole-blood T2 versus HbO2 calibration by .pdf;/home/dion/Zotero/storage/N2R42TIQ/abstract.html}
}

